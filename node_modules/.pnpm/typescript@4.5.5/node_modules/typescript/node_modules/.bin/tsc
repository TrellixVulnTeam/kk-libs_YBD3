#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/typescript@4.5.5/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
