#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/conventional-recommended-bump@6.1.0/node_modules/conventional-recommended-bump/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/conventional-recommended-bump@6.1.0/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules/conventional-recommended-bump/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/conventional-recommended-bump@6.1.0/node_modules/conventional-recommended-bump/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/conventional-recommended-bump@6.1.0/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules/conventional-recommended-bump/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-recommended-bump/cli.js" "$@"
else
  exec node  "$basedir/../conventional-recommended-bump/cli.js" "$@"
fi
