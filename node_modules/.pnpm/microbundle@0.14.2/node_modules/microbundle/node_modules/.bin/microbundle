#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules/microbundle/dist/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules/microbundle/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules/microbundle/dist/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules/microbundle/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/microbundle@0.14.2/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules/.pnpm/node_modules:/Users/huruikang/Desktop/kk-libs/node_modules:/Users/huruikang/Desktop/node_modules:/Users/huruikang/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
